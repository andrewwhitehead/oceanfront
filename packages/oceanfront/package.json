    "eslint-plugin-prettier": "^4",
    "eslint-plugin-vitest": "0.0.54",
    "eslint-plugin-vue": "^9",
    "jsdom": "~21.1",
    "prettier": "~2.8",
    "tslib": "~2.4",
    "typescript": "~4.4",
    "vite": "~4.1",
